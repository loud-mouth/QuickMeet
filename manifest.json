{
    "name" : "QuickMeet",
    "version" : "0.0.1",
    "manifest_version" : 3,
    "description" : "QuickMeet",
    "permissions" : [
        "activeTab",
        "tabs",
        "background",
        "storage",
        "contextMenus",
        "webNavigation",
        "webRequest",
        "notifications"
    ],
    "commands" : {
        "_execute_action" : {
            "suggested_key" : "Alt+M",
            "description": "Open Join Meeting Popup"
        }
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "QuickMeet",
        "default_icon": "icon128.png",
        "default_badge_text": "0",
        "default_badge_color": "#000000"
    },
    "icons" : {
        "128" : "icon128.png"
    }
}